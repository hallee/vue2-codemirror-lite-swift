{
  "name": "vue2-codemirror-lite-swift",
  "version": "1.0.2",
  "description": "Lite Codemirror component for Swift code / Vue.js 2.x",
  "main": "./dist/codemirror-swift.js",
  "module": "dist/codemirror-swift.esm.js",
  "unpkg": "dist/codemirror-swift.min.js",
  "browser": {
    "./sfc": "src/CodeMirrorSwift.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/codemirror-swift.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/codemirror-swift.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/codemirror-swift.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hallee/vue2-codemirror-lite-swift"
  },
  "keywords": [
    "vue-codemirror",
    "codemirror",
    "vue",
    "swift",
    "vue codemirror lite swift"
  ],
  "author": {
    "name": "Hal Lee",
    "url": "https://hal.codes"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hallee/vue2-codemirror-lite-swift/issues"
  },
  "homepage": "https://github.com/hallee/vue2-codemirror-lite-swift#readme",
  "dependencies": {
    "codemirror": "^5.43.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-vue": "^4.7.1",
    "vue": "^2.6.2",
    "vue-template-compiler": "^2.6.2"
  }
}
